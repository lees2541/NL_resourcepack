{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:custom_model_data",
    "index": 0,
    "on_true": {
      "type": "minecraft:select",
      "property": "minecraft:custom_model_data",
      "index": 0,
      "cases": [
        {
          "when": "explorer",
          "model": {
            "type": "minecraft:select",
            "property": "minecraft:custom_model_data",
            "index": 1,
            "cases": [
              {
                "when": "magician",
                "model": {
                  "type": "minecraft:model",
                  "model": "explorer/magician/tool/broken_orb"
                }
              }
            ]
          }
        },
        {
          "when": "roamer",
          "model": {
            "type": "minecraft:bundle/selected_item"
          }
        }
      ]
    },
    "on_false": {
      "type": "minecraft:select",
      "property": "minecraft:custom_model_data",
      "index": 0,
      "cases": [
        {
          "when": "explorer",
          "model": {
            "type": "minecraft:select",
            "property": "minecraft:custom_model_data",
            "index": 1,
            "cases": [
              {
                "when": "survivor",
                "model": {
                  "type": "minecraft:range_dispatch",
                  "property": "minecraft:custom_model_data",
                  "index": 0,
                  "scale": 1,
                  "entries": [
                    {
                      "threshold": 1,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/survivor/tool/survivor_tool"
                      }
                    },
                    {
                      "threshold": 2,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/survivor/tool/survivor_tool_x"
                      }
                    },
                    {
                      "threshold": 3,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/survivor/tool/survivor_tool_y"
                      }
                    }
                  ]
                }
              },
              {
                "when": "magician",
                "model": {
                  "type": "minecraft:range_dispatch",
                  "property": "minecraft:custom_model_data",
                  "index": 0,
                  "scale": 1,
                  "entries": [
                    {
                      "threshold": 1,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/magician/magician_tool"
                      }
                    },
                    {
                      "threshold": 2,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/magician/tool/magician_tool_x"
                      }
                    },
                    {
                      "threshold": 3,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/magician/magician_tool_y"
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "when": "roamer",
          "model": {
            "type": "minecraft:bundle/selected_item"
          }
        }
      ]
    }
  },
  "hand_animation_on_swap": false
}