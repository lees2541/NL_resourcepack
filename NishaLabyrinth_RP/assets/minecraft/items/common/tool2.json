{
    "model": {
      "type": "minecraft:condition",
      "property": "minecraft:custom_model_data",
      "index": 0,
      "on_true": {
        "type": "minecraft:empty"
      },
      "on_false": {
        "type": "minecraft:select",
        "property": "minecraft:custom_model_data",
        "index": 0,
        "cases": [
          {
            "when": "explorer",
            "model": {
              "type": "minecraft:select",
              "property": "minecraft:custom_model_data",
              "index": 1,
              "cases": [
                {
                  "when": "magician",
                  "model": {
                    "type": "minecraft:range_dispatch",
                    "property": "minecraft:custom_model_data",
                    "index": 0,
                    "scale": 1,
                    "entries": [
                      {
                        "threshold": 1,
                        "model": {
                          "type": "minecraft:range_dispatch",
                          "property": "minecraft:custom_model_data",
                          "index": 1,
                          "scale": 1,
                          "entries": [
                            {
                              "threshold": 1,
                              "model": {
                                "type": "minecraft:model",
                                "model": "minecraft:explorer/magician/tool/magician_chalk_in"
                              }
                            },
                            {
                              "threshold": 2,
                              "model": {
                                "type": "minecraft:model",
                                "model": "minecraft:explorer/magician/tool/magician_chalk_out"
                              }
                            }
                          ]
                        }
                      },
                      {
                        "threshold": 2,
                        "model": {
                          "type": "minecraft:model",
                          "model": "minecraft:explorer/magician/tool/magician_staff"
                        }
                      }
                    ]
                  }
                },
                {
                  "when": "survivor",
                  "model": {
                    "type": "minecraft:bundle/selected_item"
                  }
                }
              ]
            }
          },
          {
            "when": "roamer",
            "model": {
              "type": "minecraft:bundle/selected_item"
            }
          }
        ]
      }
    },
    "hand_animation_on_swap": false
  }