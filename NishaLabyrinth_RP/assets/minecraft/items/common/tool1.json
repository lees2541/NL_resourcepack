                    {
                      "threshold": 2,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/survivor/tool/survivor_tool_y"
                      }
                    }
                  ]
                }
              },
              {
                "when": "magician",
                "model": {
                  "type": "minecraft:range_dispatch",
                  "property": "minecraft:custom_model_data",
                  "index": 0,
                  "scale": 1,
                  "entries": [
                    {
                      "threshold": 0,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/magician/tool/magician_tool"
