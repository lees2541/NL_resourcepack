                    {
                      "threshold": 1,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/magician/tool/magician_tool_x"
                      }
                    },
                    {
                      "threshold": 2,
                      "model": {
                        "type": "minecraft:model",
                        "model": "explorer/magician/tool/magician_tool_y"
                      }
                    }
                  ],
                  "fallback": {
                    "type": "minecraft:model",
                    "model": "explorer/magician/tool/magician_tool"
                  }
                }
              }
            ]
          }
